# Import required Ruby libs
require 'rubygems'

# Import the whole of Kythera
require_relative '../lib/kythera'

# Make sure we're in Kythera's working directory
Dir.chdir File.expand_path('..', File.dirname(__FILE__))

Kythera.new

exit 0
